[
  {
    "name": "MediaWiki",
    "url": "https://www.mediawiki.org/",
    "safe": "1.38.2",
    "old_safe": "1.37.3,1.35.7",
    "vuln": "CVE-2022-34911",
    "latest": "1.38.2",
    "detection": [
      {
        "file": "DefaultSettings.php",
        "variable": "$wgVersion",
        "subdir": 1
      },
      {
        "file": "Defines.php",
        "variable": "MW_VERSION",
        "subdir": 1
      }
    ]
  },
  {
    "name": "TWiki",
    "url": "http://twiki.org/",
    "safe": "5.0.1",
    "vuln": "CVE-2010-3841",
    "detection": [
      {
        "file": "TWiki.pm",
        "variable": "$RELEASE",
        "subdir": 1
      }
    ]
  },
  {
    "name": "PmWiki",
    "url": "https://pmwiki.org/",
    "safe": "2.2.96",
    "vuln": "https://pmwiki.org/wiki/PmWiki/ChangeLog#v2296",
    "latest": "2.2.122",
    "detection": [
      {
        "file": "version.php",
        "variable": "$Version",
        "subdir": 1,
        "extra_match": "pmwiki"
      }
    ]
  },
  {
    "name": "Tiki",
    "url": "https://tiki.org/",
    "safe": "21.3",
    "vuln": "CVE-2020-29254",
    "detection": [
      {
        "file": "README",
        "variable": "version",
        "subdir": 0,
        "extra_match": "Tiki! The wiki",
        "extra_nomatch": "version 2",
        "note": "1.x"
      },
      {
        "file": "twversion.class.php",
        "variable": "$this->version",
        "subdir": 2,
        "note": "2.x and above"
      }
    ]
  },
  {
    "name": "phpwiki",
    "url": "http://phpwiki.sourceforge.net/",
    "safe": "",
    "vuln": "https://www.exploit-db.com/exploits/38027",
    "detection": [
      {
        "file": "config.php",
        "variable": "PHPWIKI_VERSION",
        "subdir": 1,
        "note": "1.2.x"
      },
      {
        "file": "prepend.php",
        "variable": "PHPWIKI_VERSION",
        "subdir": 1
      }
    ]
  },
  {
    "name": "etherpad-lite",
    "url": "https://etherpad.org/",
    "safe": "1.6.4",
    "vuln": "CVE-2018-9326",
    "detection": [
      {
        "file": "package.json",
        "variable": "version",
        "subdir": 0,
        "extra_match": "ep_etherpad-lite"
      }
    ]
  }
]
