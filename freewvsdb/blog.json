      {
        "file": "mt.php",
        "variable": "VERSION_ID",
        "subdir": 1
      }
    ]
  },
  {
    "name": "artmedic_weblog",
    "url": "http://www.artmedic-phpscripts.de/",
    "safe": "",
    "vuln": "CVE-2008-0798",
