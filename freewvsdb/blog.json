    "detection": [
      {
        "file": "artmedic_index.php",
        "variable": "# artmedic weblog",
        "subdir": 0
      }
    ]
  },
  {
    "name": "DotClear",
    "url": "https://dotclear.org/",
    "safe": "2.14.2",
    "vuln": "CVE-2018-16358",
